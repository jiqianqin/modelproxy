    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "preserveConstEnums": true,
        "removeComments": true,
        "noImplicitAny": false,
        "inlineSourceMap": true,
        "pretty": true,
        "outDir": "libs",
        "lib": ["es5", "es6", "es7", "es2017", "es2015", "es2016", "scripthost"]
    }
}